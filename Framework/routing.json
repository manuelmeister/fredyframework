{
    "routes": [
        {
            "action": "subdomainAction",
            "controller": "demo",
            "match": ":(?P<subdomain>[a-zA-Z0-9]+)\\.fredyframework:",
            "fullDomainMatch": true
        },
        {
            "action": "indexAction",
            "controller": "demo",
            "match": ":^/(Test/(?P<id>[0-9]+))?$:",
            "method": "GET"
        },
        {
            "action": "postAction",
            "controller": "demo",
            "match": ":^/(Test/(?P<id>[0-9]+))?$:",
            "method": "POST"
        },
        {
            "action": "indexAction",
            "controller": "journal",
            "match": ":^/journal/?$:"
        },
        {
            "action": "journalAction",
            "controller": "journal",
            "match": ":^/journal/(?P<id>\\d+)/?$:"
        
        },

        {
            "action": "formAction",
            "controller": "journal",
            "match": ":^/journal/new/?$:",
            "method": "GET"
        },
        {
            "action": "submitAction",
            "controller": "journal",
            "match": ":^/journal/new/?$:",
            "method": "POST"
        },
        {
            "action": "notFound",
            "controller": "error",
            "match": ":^/Error/404:"
        },
        {
            "action": "serverError",
            "controller": "error",
            "match": ":^/Error/500:"
        }
    ]
}